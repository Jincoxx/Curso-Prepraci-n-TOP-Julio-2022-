Solucion 7
function sum(...numeros) {
    let suma = 0;
    for (let i = 0; i < numeros.length; i++) {
        suma = suma + numeros[i];
    }
    return suma;
}
sum(1, 2, 3)
6
sum(8, 2)
10
sum(1, 2, 3, 4, 5)
15


Solucion 8
function multiply(a, b) {
    console.log (a * b)
}
multiply(2, 2)
4


Solucion 9
